print("Welcome to WichtSanbe-HUB")
if game:GetService("CoreGui"):FindFirstChild("WichtSanbe") then
	game:GetService("CoreGui").WichtSanbe:Destroy()
end

local gui = Instance.new("ScreenGui")
gui.Name = "WichtSanbe"
gui.ResetOnSpawn = false
gui.Parent = game:GetService("CoreGui")

local cm = 40

local mainFrame = Instance.new("Frame")
mainFrame.Size = UDim2.new(0, 12 * cm, 0, 8 * cm)
mainFrame.Position = UDim2.new(0.5, -(6 * cm), 0.5, -(4 * cm))
mainFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
mainFrame.BackgroundTransparency = 1
mainFrame.Visible = true
mainFrame.Active = true
mainFrame.Draggable = true
mainFrame.Parent = gui

local topFrame = Instance.new("Frame")
topFrame.Size = UDim2.new(0, 12 * cm, 0, 1 * cm)
topFrame.Position = UDim2.new(0, 0, 0, 0)
topFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
topFrame.BackgroundTransparency = 0.5
topFrame.Parent = mainFrame

local leftLabel = Instance.new("TextLabel")
leftLabel.Size = UDim2.new(0.5, 0, 1, 0)
leftLabel.Position = UDim2.new(0, 5, 0, 0)
leftLabel.Text = "WichtSanbe-HUB true v3 by Tuanne_"
leftLabel.TextColor3 = Color3.new(1, 1, 1)
leftLabel.TextXAlignment = Enum.TextXAlignment.Left
leftLabel.BackgroundTransparency = 1
leftLabel.Font = Enum.Font.Code
leftLabel.TextSize = 16
leftLabel.Parent = topFrame

local rightLabel = Instance.new("TextLabel")
rightLabel.Size = UDim2.new(0.5, -10, 1, 0)
rightLabel.Position = UDim2.new(0.5, 5, 0, 0)
rightLabel.Text = "[Premium]"
rightLabel.TextColor3 = Color3.new(1, 1, 1)
rightLabel.TextXAlignment = Enum.TextXAlignment.Right
rightLabel.BackgroundTransparency = 1
rightLabel.Font = Enum.Font.Code
rightLabel.TextSize = 16
rightLabel.Parent = topFrame

local bottomFrame = Instance.new("Frame")
bottomFrame.Size = UDim2.new(0, 12 * cm, 0, 7 * cm)
bottomFrame.Position = UDim2.new(0, 0, 0, 1 * cm)
bottomFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
bottomFrame.BackgroundTransparency = 0.7
bottomFrame.Parent = mainFrame

local columnA = Instance.new("Frame")
columnA.Size = UDim2.new(0, 3 * cm, 0, 7 * cm)
columnA.Position = UDim2.new(0, 0, 0, 0)
columnA.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
columnA.BackgroundTransparency = 0.7
columnA.Parent = bottomFrame

local columnB = Instance.new("Frame")
columnB.Size = UDim2.new(0, 9 * cm, 0, 7 * cm)
columnB.Position = UDim2.new(0, 3 * cm, 0, 0)
columnB.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
columnB.BackgroundTransparency = 0.7
columnB.Parent = bottomFrame

local toggleBtn = Instance.new("TextButton")
toggleBtn.Size = UDim2.new(0, 1 * cm, 0, 1 * cm)
toggleBtn.Position = UDim2.new(0, 20, 0, 60)
toggleBtn.Text = "-"
toggleBtn.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
toggleBtn.TextColor3 = Color3.new(0, 0, 0)
toggleBtn.Font = Enum.Font.SourceSans
toggleBtn.TextSize = 30
toggleBtn.Parent = gui

local visible = true
toggleBtn.MouseButton1Click:Connect(function()
	visible = not visible
	mainFrame.Visible = visible
	toggleBtn.Text = visible and "-" or "+"
end)
